{
    "description": "OOM system for Österåker GK, 2022",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "",
            "email": "william.sundberg@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "mustache/mustache": "^2.13",
        "php-di/php-di": "^6.0",
        "selective/basepath": "dev-master",
        "slim/php-view": "3.x-dev",
        "slim/psr7": "dev-master",
        "slim/slim": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
